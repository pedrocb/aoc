package main

import (
	"fmt"
)

func main() {
	var inputChar byte
	counter := 0
	for {
		_, err := fmt.Scanf("%c", &inputChar)
		if err != nil {
			break
		}
		if inputChar == '(' {
			counter += 1
		} else if inputChar == ')' {
			counter -= 1
		}
	}
	fmt.Println(counter)
}
